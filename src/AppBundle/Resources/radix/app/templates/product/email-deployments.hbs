<div class="col-md-4">
    <div class="list-group m-b-1">
        {{#link-to 'product.email-deployments.create' class='list-group-item list-group-item-action list-group-item-success'}}<i class="ion-plus-round"></i> Create{{/link-to}}
    </div>
    <div class="list-group">

        {{#each model as |item|}}
            {{#link-to 'product.email-deployments.edit' item.id class='list-group-item list-group-item-action'}}

                <small class="list-group-item-text">{{item.id}}</small>

                <h5 class="list-group-item-heading">
                    {{item.name}}
                </h5>

                <small class="list-group-item-text">{{item.createdDate}}</small>


            {{/link-to}}
        {{else}}
            <div class="list-group-item list-group-item-action">
                <p class="list-group-item-text">No results found.</p>
            </div>
        {{/each}}
    </div>
</div>
<div class="col-md-6">{{ outlet }}</div>
